[
  {
    "type": "group",
    "label": {
      "en": "Power",
      "nl": "Stroom"
    },
    "collapsed": false,
    "children": [
      {
        "id": "always_on",
        "type": "checkbox",
        "label": {
          "en": "Always on",
          "nl": "Altijd aan"
        },
        "hint": {
          "en": "This parameter determines if the device is always on. Once activated, will keep a connected device constantly on, will stop reacting to alarm frames and B-button push",
          "nl": "Deze parameter bepaalt of het apparaat altijd aan is. Wanneer geactiveerd zal de plug constant aan zijn, en niet meer reageren op de B-knop en alarm frames"
        },
        "zwave": {
          "index": 1,
          "size": 1
        },
        "value": false
      },
      {
        "id": "save_state",
        "type": "checkbox",
        "label": {
          "en": "Save Power-off State",
          "nl": "Onthoud Stroom-eraf Status"
        },
        "hint": {
          "en": "This parameter determines whether or not the device saves its last state when power is disconnected",
          "nl": "Deze parameter bepaalt of het apparaat onthoud zijn laatste status was wanneer stroom eraf word gehaald"
        },
        "zwave": {
          "index": 2,
          "size": 1
        },
        "value": true
      }
    ]
  },
